function isLeap(year) {
    
/**************Don't change the code above****************/    
    
    //Write your code here.    
    var message;
    if (year % 4 === 0){
        message = "Leap year.";
        if (year % 100 === 0){
            message = "Not leap year.";
            if (year % 400 === 0){
                message = "Leap year.";
            }
        }
    } else {
        message = "Not leap year."
    }

    return message
}
